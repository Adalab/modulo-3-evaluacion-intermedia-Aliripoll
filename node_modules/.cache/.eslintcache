e=\"addContact\"\n          type=\"text\"\n          name=\"name\"\n          id=\"name\"\n          onInput={handleSearch}\n        />  \n        </form> \n      <table className=\"table\">\n        <thead>\n          <tr>\n            <th>Nombre</th>\n            <th>Tutora</th>\n            <th>Especialidad</th>\n          </tr>\n        </thead>\n        <tbody>\n      {htmlData}\n        </tbody>\n      </table>\n      <section className='section'>\n        <h2>Añadir una nueva Adalaber</h2>\n        <form>\n          <label htmlFor=\"name\">Nombre:</label>\n          <input \n           type=\"text\"\n           name=\"name\" \n           id=\"name\"\n           onInput={handleNewContact} />\n          <label htmlFor=\"counselor\">Tutora:</label>\n          <input \n          type=\"text\" \n          name=\"counselor\" \n          id=\"counselor\"\n          onInput={handleNewContact}/>\n        \n          <label htmlFor=\"speciality\">Especialidad:</label>\n          <input \n          type=\"text\" \n          name=\"speciality\" \n          id=\"speciality\"\n          onInput={handleNewContact} />\n        </form>\n        <button className='section__btn' onClick={handleClick}>Añadir una nueva Adalaber</button>\n\n      </section>\n\n    </div>\n  );\n}\n\nexport default App;\n",{"ruleId":"14","severity":1,"message":"15","line":9,"column":10,"nodeType":"16","messageId":"17","endLine":9,"endColumn":16},"no-unused-vars","'search' is assigned a value but never used.","Identifier","unusedVar"]